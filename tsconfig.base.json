{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "baseUrl": ".",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "paths": {
      "@spacetraders/create-agent": [
        "libs/components/create-agent-form/src/index.ts"
      ],
      "@spacetraders/chakra-provider": [
        "libs/components/chakra-provider/src/index.ts"
      ],
      
      "@spacetraders/core-spacetraders": [
        "libs/core/spacetraders/src/index.ts"
      ],
      "@spacetraders/core-spacetraders/register": [
        "libs/core/spacetraders/src/lib/register-agent.ts"
      ]
    }
  }
}
