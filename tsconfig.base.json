{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "baseUrl": ".",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "paths": {
      "@spacetraders/api-create-agent": [
        "libs/api/create-agent-module/src/index.ts"
      ],
      "@spacetraders/api-create-agent/register": [
        "libs/api/create-agent-module/src/lib/register-agent.ts"
      ],
      "@spacetraders/chakra-provider": [
        "libs/components/chakra-provider/src/index.ts"
      ],
      "@spacetraders/create-agent": [
        "libs/components/create-agent-form/src/index.ts"
      ],
    }
  }
}
