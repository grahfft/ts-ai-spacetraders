{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "baseUrl": ".",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "paths": {
      "@spacetraders/chakra-provider": [
        "libs/components/chakra-provider/src/index.ts"
      ],
      "@spacetraders/create-agent": [
        "libs/components/create-agent-form/src/index.ts"
      ],
      "services": ["libs/services/spacetraders/src/index.ts"]
    }
  }
}
