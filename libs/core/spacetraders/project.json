{
  "name": "core-spacetraders",
  "root": "libs/core/spacetraders",
  "sourceRoot": "libs/core/spacetraders/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{workspaceRoot}/dist/libs/core/spacetraders"],
      "options": {
        "platform": "node",
        "format": ["cjs", "esm"],
        "outputPath": "dist/libs/core/spacetraders",
        "main": "libs/core/spacetraders/src/index.ts",
        "tsConfig": "libs/core/spacetraders/tsconfig.lib.json",
        "bundle": false,
        "external": ["axios"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["libs/core/spacetraders/src/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/core/spacetraders"],
      "options": {
        "jestConfig": "libs/core/spacetraders/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["scope:core", "type:lib"]
}


