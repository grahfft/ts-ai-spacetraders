{
  "name": "api",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/api/agents-module/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/api/agents-module",
        "main": "libs/api/agents-module/src/index.ts",
        "tsConfig": "libs/api/agents-module/tsconfig.lib.json",
        "assets": []
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "libs/api/agents-module/src/**/*.ts"
        ],
        "fix": true,
        "ignorePath": ".eslintignore"
      }
    }
  }
}
