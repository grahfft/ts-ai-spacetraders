# Space Traders API Configuration
SPACE_TRADERS_API_TOKEN=your_api_token_here
SPACE_TRADERS_API_BASE_URL=https://api.spacetraders.io/v2

# Optional: Account token used to authorize registrations (do not commit real value)
SPACE_TRADERS_ACCOUNT_TOKEN=

# Optional: External service base URL (Nest service)
NEXT_PUBLIC_SERVICE_URL=http://localhost:3001/api

# Agent Configuration
AGENT_NAME=TS-AI-Agent
AGENT_FACTION=COSMIC
AGENT_SYMBOL=TSAI

# Logging
LOG_LEVEL=info
DEBUG=false

# Trading Configuration
MAX_SHIPS=10
INITIAL_CREDITS=100000
RISK_TOLERANCE=medium

# Postgres
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=spacetraders
POSTGRES_USER=spacetraders
POSTGRES_PASSWORD=spacetraders
